model Task {
  id String @id @default(uuid())
  userId        String   
  title         String
  description   String
  status        Status   @default(incomplete)
  completedDate DateTime?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  user User  @relation(fields: [userId], references: [id])
}

enum Status {
  incomplete
  inProgress
  complete
}
